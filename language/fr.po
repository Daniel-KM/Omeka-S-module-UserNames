#: view/common/admin/username-detail.phtml:6
#: view/common/admin/username-show.phtml:6
msgid "User name"
msgstr "Identifiant utilisateur"

#: Module.php:251 src/Api/Adapter/UserNameAdapter.php:63
#, php-format
msgid "The user name %s is already taken."
msgstr "L'identifiant utilisateur %s est déjà pris."

#: src/Form/LoginForm.php:15
msgid "User name or email"
msgstr "Identifiant ou e-mail"

#: src/Controller/LoginController.php:40
msgid "User name, email, or password is invalid"
msgstr "L'identifiant utilisateur, l'e-mail ou le mot de passe est invalide."

#: src/Api/Adapter/UserNameAdapter.php:69
msgid "The user name cannot be empty."
msgstr "L'identifiant utilisateur ne peut pas être vide."

#: Module.php:267 src/Api/Adapter/UserNameAdapter.php:79
#, php-format
msgid "User name must be between %1$s and %2$s characters."
msgstr "L'identifiant utilisateur doit contenir entre %1$s et %2$s caractères."

#: Module.php:276 src/Api/Adapter/UserNameAdapter.php:87
#, php-format
msgid ""
"Whitespace is not allowed. Only these special characters may be used: %s"
msgstr ""
"Les espaces ne sont pas autorisés dans l'identifiant utilisateur. Utilisez "
"uniquement les caractères spéciaux suivants : %s"

#: src/Form/ConfigForm.php:20
msgid "User name minimum length"
msgstr "Longueur minimum des identifiants utilisateur"

#: src/Form/ConfigForm.php:34
msgid "User name maximum length"
msgstr "Longueur maximum des identifiants utilisateurs"

#: Module.php:140
msgid "Minimum length cannot be empty."
msgstr "La taille minimum ne peut pas être vide"

#: Module.php:148
msgid "Maximum length cannot be empty."
msgstr "La taille maximum ne peut pas être vide"

#: Module.php:156
msgid "Max and min length out of bounds. Maximum length cannot be over 190."
msgstr "Taille minimum et/ou maximum hors limite. La taille maximum ne peut "
"pas dépasser 190."
