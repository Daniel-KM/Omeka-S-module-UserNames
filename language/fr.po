#, php-format
msgid "The user name %s is already taken."
msgstr "L'identifiant utilisateur %s est déjà pris."

#: src/Form/LoginForm.php:15
msgid "User name or email"
msgstr "Identifiant ou e-mail"

#: src/Controller/LoginController.php:40
msgid "User name, email, or password is invalid"
msgstr "L'identifiant utilisateur, l'e-mail our le mot de passe est invalide."

#: src/Api/Adapter/UserNameAdapter.php:69
msgid "The user name cannot be empty."
msgstr "L'identifiant utilisateur ne peut pas être vide."

